<div class="container col-xs-12 col-sm-8 col-sm-offset-2">
	{% for post in posts %}
		<div class="post">
			<div class="post-subject">
				<h2><a href="/viewpost/{{post.key().id()}}">{{ post.subject }}</a></h2>
			</div>
			<div class="post-content">{{ post.content }}</div>
			<hr>
		</div>
	{% endfor %}
</div>